<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>LYSR | <%=title%></title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.1/css/all.min.css">
    <link rel="stylesheet" href="/css/style.css">
    <link rel="stylesheet" href="/bootstrap4/css/bootstrap.min.css">
    <link rel="stylesheet" href="/css/footer.css">
    <link rel="stylesheet" href="/css/landing.css">
    <link rel="stylesheet" href="/css/aos.css">
    <link rel="stylesheet" href="/css/nav.css">
</head>

<body>
    <%- include('./partials/nav.ejs') %>
    <section class="ly-hide-small ly-hide-medium">
        <div class="container-box">
            <img src="image/landing.png" alt="" width="100%" height="auto">
            <div class="top-left">
                <div class="floating-div" data-aos='fade-down'>
                    <h1 class="border-text">Lycan</h1>
                    <div data-aos='fade-left' style="transition-delay: 100ms;">
                        <h2 class="title">Space Research</h2>
                    </div>
                </div>
            </div>
            <div class="bottom-right" data-aos='slide-up' style="transition-delay: 200ms;">
                <h2 class="bottom-text">
                    Go Beyond.
                </h2>
            </div>
        </div>
    </section>
    <section class="ly-hide-large ly-hide-xlarge ly-hide-small">
        <div class="container-box">
            <img src="image/mob/landing.png" alt="" width="100%" height="auto">
            <div class="top-left">
                <div class="floating-div">
                    <h1 class="border-text">Lycan</h1>
                    <div>
                        <h2 class="title">Space Research</h2>
                    </div>
                </div>
            </div>
            <div class="bottom-right">
                <h2 class="bottom-text">
                    Go Beyond.
                </h2>
            </div>
        </div>
    </section>
    <section class="ly-hide-large ly-hide-xlarge ly-hide-medium">
        <div class="container-box">
            <img src="image/mob/landing.png" alt="" width="100%" height="auto">
            <div class="top-left">
                <div class="floating-div">
                    <h1 class="border-text">Lycan</h1>
                    <div>
                        <h2 class="title">Space Research</h2>
                    </div>
                </div>
            </div>
            <div class="bottom-right">
                <h2 class="bottom-text">
                    Go Beyond.
                </h2>
            </div>
        </div>
    </section>
    <%- include('./partials/footer.ejs') %>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    <script src="/js/aos.js"></script>
    <script>
        AOS.init({
            duration: 1200,
        })
    </script>
</body>

</html>